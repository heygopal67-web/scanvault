{
  "expo": {
    "name": "scanvault",
    "slug": "scanvault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoo.png",
    "logo": "./assets/images/logoo.png",
    "scheme": "scanvault",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoo.png",
        "backgroundColor": "#FAF3E3"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gopalkumar.scanvault"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logoo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logoo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d0906711-a73e-4ce5-9580-d00487fb6946"
      }
    }
  }
}
